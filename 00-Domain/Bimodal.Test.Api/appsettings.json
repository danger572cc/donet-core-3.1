{
    "ConnectionStrings": {
        "DomainConnection": "Data Source=domain-store.db;",
        "EventsConnection": "Data Source=event-store.db;"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "JwtTokenConfig": {
        "accessTokenExpiration": 20,
        "audience": "https://mywebapi.com",
        "issuer": "https://mywebapi.com",
        "secret": "1234567890123456789"
    }
}
